{
  "version": "0.2",
  "language": "en",
  "files": ["**/*.{md,mdx,ts,tsx,js,jsx,json}"],
  "ignorePaths": ["package-lock.json", "yarn.lock", "*.log"],
  "ignoreRegExpList": ["/```[\\s\\S]*?```/", "/`[^`]*`/"],
  "allowCompoundWords": true,
  "dictionaries": ["en", "en-gb", "softwareTerms", "typescript", "node", "npm", "html", "css", "markdown"],
  "useGitignore": true,
  "caseSensitive": false,
  "minWordLength": 3,
  "words": [
    "bacalhau",
    "docusaurus",
    "mdx",
    "swizzle",
    "wrangler",
    "clsx",
    "prism",
    "tsconfig",
    "ipfs",
    "expanso",
    "idx",
    "boinc",
    "desci",
    "GTM",
    "cuda",
    "milli",
    "millicores",
    "webui",
    "libp2p",
    "duckdb",
    "myvalue",
    "oom"
  ]
}
